{
  "ssh": {
    "command": "systemctl status ssh",
    "recover": "systemctl restart ssh"
  },
  "dhcp": {
    "command": "systemctl status isc-dhcp-server",
    "recover": "systemctl restart isc-dhcp-server"
  },
  "cloudflare-ping": {
    "command": "ping -W 2 -c 1 1.1.1.1"
  },
  "resolve-google": {
    "command": "ping -W 2 -c 1 google.com"
  }
}